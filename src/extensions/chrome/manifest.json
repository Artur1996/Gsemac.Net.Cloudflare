{
    "manifest_version": 2,
    "name": "Stealth",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "run_at": "document_start",
            "all_frames": true,
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "stealth.js"
    ]
}